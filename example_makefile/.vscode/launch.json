    "version": "0.2.0",
    "configurations": [
        {
            "name": "UT Debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "D:/STM32_Teo/UT_InEmb/build/UnitTests/UTexe/SampleFileToUT_UnitTests.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
