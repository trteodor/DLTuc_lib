            "MIMode": "gdb",
            "miDebuggerPath": "gdb",
            "setupCommands": [
                {
                    "description": "Włącz formatowanie kodu dla gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "Cortex Debug",
            "cwd": "${workspaceRoot}",
            "executable": "./.build_debug/CompilationOutputFiles/DltLogLibTests/DltLogLibTests.elf",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "openocd",
            "device": "STM32F103RB",
            "svdFile": "STM32F103xx.svd"
            "configFiles": [
                "interface/stlink.cfg",
                "target/stm32f1x.cfg"
            ],
            "interface": "swd",
            "runToMain": true,
        }
